{
  "compilerOptions": {
   
    "target": "ES2016",
    "module": "commonjs",
    "noEmitOnError": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "skipLibCheck": true,
    "removeComments": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true
    }, 
    "ts-node": {
      "esm": true
    }, 
     
}
